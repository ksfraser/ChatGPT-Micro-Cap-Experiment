{
    "name": "ksfraser/stock-info",
    "description": "Legacy Stock Information Models for historical stock data analysis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "K.S. Fraser",
            "email": "ksfraser@example.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ksfraser\\StockInfo\\": "src/Ksfraser/StockInfo/",
            "Ksfraser\\MotleyFool\\": "src/Ksfraser/MotleyFool/",
            "Ksfraser\\WarrenBuffett\\": "src/Ksfraser/WarrenBuffett/",
            "Ksfraser\\IPlace\\": "src/Ksfraser/IPlace/",
            "Ksfraser\\Evaluation\\": "src/Ksfraser/Evaluation/",
            "Ksfraser\\Analysis\\": "src/Ksfraser/Analysis/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
